  , "methods" : "shell-application/shell-application" 
  }
, {
    "name" : "layouts"
  , "path" : "/layouts/{id}"
  , "methods" : "app-loader/app-loader" 
  }
, {
    "name" : "platform-app-explorer-legacy"
  , "path" : "/platform/app-explorer(/params/{params})"
  , "methods" : "app-loader/app-loader" 
  }
, {
    "name" : "execution-exec-market-rates-app-legacy"
  , "path" : "/execution/exec-market-rates-app(/params/{params})"
  , "methods" : "app-loader/app-loader" 
  }
, {
    "name" : "people"
  , "path" : "/people"
  , "methods" : "people/people"
  }
, {
    "name" : "person"
  , "path" : "/people/{id}"
  , "methods" : {
      "get" : {
        "application/x.nap.view" : [
          { "div" : "people/full" }
